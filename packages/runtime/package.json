{
	"type": "module",
	"name": "@intrnl/velvet",
	"version": "0.2.1",
	"author": "intrnl",
	"license": "MIT",
	"repository": {
		"url": "https://codeberg.org/intrnl/velvet",
		"directory": "packages/runtime"
	},
	"sideEffects": false,
	"files": [
		"dist/"
	],
	"exports": {
		".": {
			"import": "./dist/esm/public.js",
			"require": "./dist/cjs/public.js"
		},
		"./internal": {
			"import": "./dist/esm/internal.js",
			"require": "./dist/cjs/internal.js"
		},
		"./store": {
			"import": "./dist/esm/store.js",
			"require": "./dist/cjs/store.js"
		}
	},
	"scripts": {
		"prepublishOnly": "rm -r dist; pnpm run bundle",
		"bundle": "rollup -c"
	},
	"devDependencies": {
		"esbuild": "^0.14.38",
		"rollup": "^2.74.1"
	}
}
