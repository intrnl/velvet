{
	"type": "module",
	"name": "@intrnl/velvet",
	"version": "0.5.0",
	"author": "intrnl",
	"license": "MIT",
	"repository": {
		"url": "https://codeberg.org/intrnl/velvet",
		"directory": "packages/runtime"
	},
	"sideEffects": false,
	"files": [
		"dist/"
	],
	"exports": {
		".": {
			"import": "./dist/esm/public.js",
			"require": "./dist/cjs/public.js"
		},
		"./internal": {
			"import": "./dist/esm/internal.js",
			"require": "./dist/cjs/internal.js"
		},
		"./macro": {
			"import": "./dist/esm/macro.js",
			"require": "./dist/cjs/macro.js"
		}
	},
	"scripts": {
		"prepublishOnly": "pnpm run build",
		"build": "rm -r dist; rollup -c",
		"watch": "rollup -cw",
		"test": "mocha --recursive"
	},
	"devDependencies": {
		"esbuild": "^0.17.14",
		"mocha": "^10.2.0",
		"nanospy": "^0.5.0",
		"rollup": "^3.20.2"
	}
}
